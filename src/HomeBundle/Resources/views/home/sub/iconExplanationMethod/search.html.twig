<script>
    var searchIconDiv = document.getElementById('searchIconDiv');
    var searchIconContent = document.getElementById('searchIconContent');
    
    searchIconDiv.addEventListener("mouseover", function(){mouseover(fieldName="searchIconContent", event="mouseover");});
    searchIconDiv.addEventListener("mouseout", function(){mouseout(fieldName="searchIconContent", event="mouseout");});
    searchIconDiv.addEventListener("click", function(){click(fieldName="searchIconContent", event="click");});
</script>