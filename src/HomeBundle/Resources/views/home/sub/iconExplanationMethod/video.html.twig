<script>
    var videoIconDiv = document.getElementById('videoIconDiv');
    var videoIconContent = document.getElementById('videoIconContent');

    videoIconDiv.addEventListener("mouseover", function(){mouseover(fieldName="videoIconContent", event="mouseover");});
    videoIconDiv.addEventListener("mouseout", function(){mouseout(fieldName="videoIconContent", event="mouseout");});
    videoIconDiv.addEventListener("click", function(){click(fieldName="videoIconContent", event="click");});
</script>