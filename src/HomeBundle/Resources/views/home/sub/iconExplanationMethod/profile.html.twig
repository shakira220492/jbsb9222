<script>

    var profileIconDiv = document.getElementById('profileIconDiv');
    var profileIconContent = document.getElementById('profileIconContent');

    profileIconDiv.addEventListener("mouseover", function(){mouseover(fieldName="profileIconContent", event="mouseover");});
    profileIconDiv.addEventListener("mouseout", function(){mouseout(fieldName="profileIconContent", event="mouseout");});
    profileIconDiv.addEventListener("click", function(){click(fieldName="profileIconContent", event="click");});
</script>