<script>
    var configurationIconDiv = document.getElementById('configurationIconDiv');
    var configurationIconContent = document.getElementById('configurationIconContent');

    configurationIconDiv.addEventListener("mouseover", function(){mouseover(fieldName="configurationIconContent", event="mouseover");});
    configurationIconDiv.addEventListener("mouseout", function(){mouseout(fieldName="configurationIconContent", event="mouseout");});
    configurationIconDiv.addEventListener("click", function(){click(fieldName="configurationIconContent", event="click");});
</script>