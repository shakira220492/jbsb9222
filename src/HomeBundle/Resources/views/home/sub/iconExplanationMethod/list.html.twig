<script>
    var listIconDiv = document.getElementById('listIconDiv');
    var listIconContent = document.getElementById('listIconContent');

    listIconDiv.addEventListener("mouseover", function(){mouseover(fieldName="listIconContent", event="mouseover");});
    listIconDiv.addEventListener("mouseout", function(){mouseout(fieldName="listIconContent", event="mouseout");});
    listIconDiv.addEventListener("click", function(){click(fieldName="listIconContent", event="click");});
</script>